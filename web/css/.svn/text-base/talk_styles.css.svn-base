/* CSS Document */

body {padding:1em;}
h1, h3, h4, h5 {color:#444;}
h1 {border-bottom:1px solid #999999; padding:1em 0 .1em 0;}
h2 { color:#5A87CF; border-bottom:1px solid #cccccc; padding:.1em 0 .1em 0; margin-bottom:1em;}
h4 {font-size:100%; padding-bottom:0px; margin-bottom:0px;}

.center { margin-left:auto; margin-right:auto; text-align:center;}
.header {padding-bottom:1em;}
.nav {list-style:none; padding-left:5px;}
.nav .sel {text-decoration:none; color:#000; font-weight:bold;}
.footer {text-align:center; border-top:1px solid #ccc; margin-top:2em; padding-top:1em;}
.cb {clear:both;}